<?xml version="1.0"?>
<block>
  <name>traffic_parser</name>
  <key>hurdle1_traffic_parser</key>
  <category>[hurdle1]</category>
  <import>import hurdle1</import>
  <make>hurdle1.traffic_parser($len_tag_name, $zero_pad_tag_name)</make>
  <param>
    <name>Length Tag Name</name>
    <key>len_tag_name</key>
    <type>string</type>
  </param>
  <param>
    <name>Zero Pad Tag Name</name>
    <key>zero_pad_tag_name</key>
    <type>string</type>
  </param>  
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
